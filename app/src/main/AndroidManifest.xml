<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.teaming.www.teaming">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- 어플리케이션 등록. 로드를 하기전에 실행이 된다. -->
    <application
        android:name=".common.app.TeamingApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controller.main.MainActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.study.detail.StudyDetailActivity" />
        <activity android:name=".controller.notice.NoticeDetailsActivity" />
        <activity android:name=".controller.matching.detail.MatchingDetailActivity"></activity>
    </application>

</manifest>